<?xml version="1.0"?>
<launch>
  <include file="$(find kurt_gazebo)/launch/upload_kurt.launch"/>

  <!-- start gazebo with the willow world -->
  <param name="/use_sim_time" value="true" />

  <node name="gazebo" pkg="gazebo" type="gazebo" args="-u $(find gazebo_worlds)/worlds/wg_collada.world" respawn="false" output="screen"/>

  <include file="$(find kurt_gazebo)/launch/robot.launch"/>
</launch>
